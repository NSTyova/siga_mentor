[32m+[m		[32mMunicipio municipio = new Municipio();[m
[32m+[m		[32mmunicipio.setDescricao(descricao);[m
[32m+[m		[32mmunicipio.setProvincia(provincia);[m
[32m+[m		[32mreturn municipio;[m
[32m+[m	[32m}[m
[32m+[m[32m}[m
