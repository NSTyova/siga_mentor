[32m+[m[32m// processa o auto-complete de especialidades[m
[32m+[m[32m$(function() {[m
[32m+[m	[32m// remove o espaco depois da virgula[m
[32m+[m	[32mfunction split(val) {[m
[32m+[m		[32mreturn val.split(/,\s*/);[m
[32m+[m	[32m}[m
[32m+[m
[32m+[m	[32m// extrai o ultimo termo adicionado[m
[32m+[m	[32mfunction extractLast(term) {[m
[32m+[m		[32mreturn split(term).pop();[m
[32m+[m	[32m}[m
[32m+[m[41m	[m
[32m+[m	[32m// adicioana a tag de input com a especializacao no html[m[41m [m
[32m+[m	[32mfunction addEspecializacao(titulo) {[m
[32m+[m		[32m$('#listaEspecializacoes')[m
[32m+[m			[32m.append('<input type="hidden" value="'+ titulo +'" name="especialidades">');[m
[32m+[m	[32m}[m
[32m+[m[41m	[m
[32m+[m	[32m// mostra na pagina um toast com mensagem de especialidades repetidas[m[41m [m
[32m+[m[32m    function exibeMessagem(texto) {[m
[32m+[m[32m        $('.add-toast').append(""[m
[32m+[m[32m          .concat('<div class="toast" role="alert" aria-live="assertive" aria-atomic="true" data-delay="2800">',[m
[32m+[m[32m                  '<div class="toast-header">',[m
[32m+[m[32m                  '<strong class="mr-auto">Atenção</strong>',[m
[32m+[m[32m                  '<button type="button" class="ml-2 mb-1 close" data-dismiss="toast" aria-label="Close">',[m
[32m+[m[32m                  '  <span aria-hidden="true">&times;</span>',[m
[32m+[m[32m                  '</button>',[m
[32m+[m[32m              '</div>',[m
[32m+[m[32m              '<div class="toast-body">', texto, '</div>',[m
[32m+[m[32m          '</div>')[m
[32m+[m[32m        );[m
[32m+[m[32m        $('.toast').toast('show');[m
[32m+[m[32m        $('.toast').on('hidden.bs.toast', function (e) {[m
[32m+[m[32m            $(e.currentTarget).remove();[m
[32m+[m[32m        });[m
[32m+[m[32m    }[m[41m	[m
[32m+[m
[32m+[m	[32m$("#autocomplete-especialidades")[m
[32m+[m		[32m.on("keydown",	function(event) {[m
[32m+[m			[32mif (event.keyCode === $.ui.keyCode.TAB[m
[32m+[m					[32m&& $(this).autocomplete("instance").menu.active) {[m
[32m+[m				[32mevent.preventDefault();[m
[32m+[m			[32m}[m
[32m+[m		[32m})[m
[32m+[m		[32m.autocomplete({[m
[32m+[m			[32msource : function(request, response) {[m
[32m+[m				[32m$.getJSON("/especialidades/titulo", {[m
[32m+[m					[32mtermo : extractLast(request.term)[m
[32m+[m				[32m}, response);[m
[32m+[m			[32m},[m
[32m+[m			[32msearch : function() {[m
[32m+[m				[32m// custom minLength[m
[32m+[m				[32mvar term = extractLast(this.value);[m
[32m+[m				[32mif (term.length < 1) {[m
[32m+[m					[32mreturn false;[m
[32m+[m				[32m}[m
[32m+[m			[32m},[m
[32m+[m			[32mfocus : function() {[m
[32m+[m				[32m// prevent value inserted on focus[m
[32m+[m				[32mreturn false;[m
[32m+[m			[32m},[m
[32m+[m			[32mselect : function(event, ui) {[m
[32m+[m				[32mvar terms = split(this.value);[m
[32m+[m				[32mconsole.log('1. this.value: ' + this.value)[m
[32m+[m				[32mconsole.log('2. terms: ' + terms)[m
[32m+[m				[32mconsole.log('3. ui.item.value: ' + ui.item.value)[m
[32m+[m				[32m// remove a entrada atual[m
[32m+[m				[32mterms.pop();[m
[32m+[m				[32mconsole.log('4. terms: ' + terms)[m
[32m+[m				[32m// testa se valor já foi inserido no array[m
[32m+[m				[32mvar exists = terms.includes(ui.item.value);[m[41m				[m
[32m+[m				[32mif (exists === false) {[m[41m				[m
[32m+[m					[32m// add the selected item[m
[32m+[m					[32mterms.push(ui.item.value);[m
[32m+[m					[32mconsole.log('5. terms: ' + terms)[m
[32m+[m					[32mterms.push("");[m
[32m+[m					[32mconsole.log('6. terms: ' + terms)[m
[32m+[m					[32mthis.value = terms.join(", ");[m
[32m+[m					[32mconsole.log('7. this.value: ' + this.value)[m
[32m+[m					[32mconsole.log('8. ui.item.value: ' + ui.item.value)[m
[32m+[m					[32m// adiciona especializacao na pagina para envio ao controller[m
[32m+[m					[32maddEspecializacao(ui.item.value);[m
[32m+[m				[32m} else {[m
[32m+[m					[32mexibeMessagem('A Especialização <b>'+ ui.item.value +'</b> já foi selecionada.');[m
[32m+[m				[32m}[m
[32m+[m				[32mreturn false;[m
[32m+[m			[32m}[m
[32m+[m		[32m});[m
[32m+[m[32m});[m
