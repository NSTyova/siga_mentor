[32m+[m[32mpackage ao.co.temmais.arquitetura.modelo;[m
[32m+[m
[32m+[m[32mimport java.util.ArrayList;[m
[32m+[m[32mimport java.util.List;[m
[32m+[m
[32m+[m[32mimport javax.persistence.Column;[m
[32m+[m[32mimport javax.persistence.Entity;[m
[32m+[m[32mimport javax.persistence.GeneratedValue;[m
[32m+[m[32mimport javax.persistence.GenerationType;[m
[32m+[m[32mimport javax.persistence.Id;[m
[32m+[m[32mimport javax.persistence.JoinColumn;[m
[32m+[m[32mimport javax.persistence.JoinTable;[m
[32m+[m[32mimport javax.persistence.ManyToMany;[m
[32m+[m[32mimport javax.persistence.Table;[m
[32m+[m
[32m+[m[32m@SuppressWarnings("serial")[m
[32m+[m[32m@Entity[m
[32m+[m[32m@Table(name = "usuario")[m
[32m+[m[32mpublic class Usuario {[m
[32m+[m	[32m@Id[m
[32m+[m	[32m@GeneratedValue(strategy = GenerationType.IDENTITY)[m
[32m+[m	[32mprivate Long id;[m
[32m+[m	[32m@Column(name = "nome_usuario")[m
[32m+[m	[32mprivate String nome;[m
[32m+[m	[32m@Column(name = "email", unique = true, nullable = false)[m
[32m+[m	[32mprivate String email;[m
[32m+[m	[32m@Column(name = "senha", nullable = false)[m
[32m+[m	[32mprivate String senha;[m
[32m+[m
[32m+[m	[32m@Column(name = "ativo", nullable = false, columnDefinition = "TINYINT(1)")[m
[32m+[m	[32mprivate boolean ativo;[m
[32m+[m
[32m+[m	[32m@Column(name = "codigo_verificador", length = 6)[m
[32m+[m	[32mprivate String codigoVerificador;[m
[32m+[m
[32m+[m	[32m@ManyToMany[m
[32m+[m	[32m@JoinTable(name = "usuarios_tem_perfis", joinColumns = {[m
[32m+[m			[32m@JoinColumn(name = "usuario_id", referencedColumnName = "id") }, inverseJoinColumns = {[m
[32m+[m					[32m@JoinColumn(name = "perfis_id", referencedColumnName = "id") })[m
[32m+[m	[32mprivate List<Perfil> perfis;[m
[32m+[m
[32m+[m	[32m// adiciona perfis a lista[m
[32m+[m	[32mpublic void addPerfil(PerfilTipo tipo) {[m
[32m+[m		[32mif (this.perfis == null) {[m
[32m+[m			[32mthis.perfis = new ArrayList<>();[m
[32m+[m		[32m}[m
[32m+[m		[32mthis.perfis.add(new Perfil(tipo.getCod()));[m
[32m+[m	[32m}[m
[32m+[m
[32m+[m	[32mpublic Usuario() {[m
[32m+[m		[32msuper();[m
[32m+[m	[32m}[m
[32m+[m
[32m+[m	[32mpublic Usuario(String email) {[m
[32m+[m		[32mthis.email = email;[m
[32m+[m	[32m}[m
[32m+[m
[32m+[m	[32mpublic String getEmail() {[m
[32m+[m		[32mreturn email;[m
[32m+[m	[32m}[m
[32m+[m
[32m+[m	[32mpublic void setEmail(String email) {[m
[32m+[m		[32mthis.email = email;[m
[32m+[m	[32m}[m
[32m+[m
[32m+[m	[32mpublic String getSenha() {[m
[32m+[m		[32mreturn senha;[m
[32m+[m	[32m}[m
[32m+[m
