[32m+[m[32mpackage ao.co.temmais.arquitetura.controller.converter;[m
[32m+[m
[32m+[m[32mimport java.util.HashSet;[m
[32m+[m[32mimport java.util.Set;[m
[32m+[m
[32m+[m[32mimport org.springframework.beans.factory.annotation.Autowired;[m
[32m+[m[32mimport org.springframework.core.convert.converter.Converter;[m
[32m+[m[32mimport org.springframework.stereotype.Component;[m
[32m+[m
[32m+[m[32mimport ao.co.temmais.arquitetura.modelo.Perguntas;[m
[32m+[m[32mimport ao.co.temmais.arquitetura.repository.PerguntasRepository;[m
[32m+[m
[32m+[m
[32m+[m[32m@Component[m
[32m+[m[32mpublic class PerguntasConverter implements Converter<String[], Set<Perguntas>> {[m
[32m+[m
[32m+[m	[32m@Autowired[m
[32m+[m	[32mprivate PerguntasRepository repository;[m
[32m+[m
[32m+[m	[32m@Override[m
[32m+[m	[32mpublic Set<Perguntas> convert(String[] descricao) {[m
[32m+[m[41m		[m
[32m+[m		[32mSet<Perguntas> perguntas = new HashSet<>();[m
[32m+[m		[32mif (descricao != null && descricao.length > 0) {[m
[32m+[m			[32mperguntas.addAll(repository.findAll());[m[41m			[m
[32m+[m		[32m}[m
[32m+[m		[32mreturn perguntas;[m
[32m+[m	[32m}[m
[32m+[m[32m}[m
