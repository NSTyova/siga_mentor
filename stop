[32m+[m[32mpackage ao.co.temmais.arquitetura.controller.config;[m
[32m+[m
[32m+[m
[32m+[m[32mimport org.springframework.beans.factory.annotation.Autowired;[m
[32m+[m[32mimport org.springframework.security.config.annotation.authentication.builders.AuthenticationManagerBuilder;[m
[32m+[m[32mimport org.springframework.security.config.annotation.web.builders.HttpSecurity;[m
[32m+[m[32mimport org.springframework.security.config.annotation.web.configuration.EnableWebSecurity;[m
[32m+[m[32mimport org.springframework.security.config.annotation.web.configuration.WebSecurityConfigurerAdapter;[m
[32m+[m[32mimport org.springframework.security.crypto.bcrypt.BCryptPasswordEncoder;[m
[32m+[m
[32m+[m[32mimport ao.co.temmais.arquitetura.modelo.PerfilTipo;[m
[32m+[m[32mimport ao.co.temmais.arquitetura.repository.service.UsuarioService;[m
[32m+[m
[32m+[m[32m@EnableWebSecurity[m
[32m+[m[32mpublic class SecurityConfig extends WebSecurityConfigurerAdapter{[m
[32m+[m
[32m+[m
[32m+[m	[32mprivate static final String ADMIN = PerfilTipo.ADMIN.getDesc();[m
[32m+[m	[32mprivate static final String ARQUITETOS = PerfilTipo.ARQUITETOS.getDesc();[m
[32m+[m	[32mprivate static final String CANDIDATOS = PerfilTipo.CANDIDATOS.getDesc();[m
[32m+[m	[32mprivate static final String SECRETARIA = PerfilTipo.SECRETARIA.getDesc();[m
[32m+[m[41m   [m
[32m+[m[41m	[m
[32m+[m	[32m@Autowired[m
[32m+[m	[32mprivate UsuarioService service;[m
[32m+[m
[32m+[m	[32m@Override[m
[32m+[m	[32mprotected void configure(HttpSecurity http) throws Exception {[m
[32m+[m[41m		[m
[32m+[m		[32mhttp.authorizeRequests()[m
[32m+[m			[32m// acessos públicos liberados[m
[32m+[m			[32m.antMatchers("/webjars/**", "/css/**", "/image/**", "/js/**").permitAll()[m
[32m+[m			[32m.antMatchers("/", "/home").permitAll()[m
[32m+[m			[32m.antMatchers("/u/novo/cadastro", "/u/cadastro/realizado", "/u/cadastro/paciente/salvar").permitAll()[m
[32m+[m[41m			[m
[32m+[m			[32m// acessos privados admin[m
[32m+[m			[32m.antMatchers("/u/editar/senha", "/u/confirmar/senha").hasAnyAuthority(ARQUITETOS, CANDIDATOS)[m
[32m+[m			[32m.antMatchers("/u/**").hasAuthority(ADMIN)[m
[32m+[m[41m			[m
[32m+[m			[32m// acessos privados medicos[m
[32m+[m			[32m.antMatchers("/medicos/especialidade/titulo/*").hasAnyAuthority(SECRETARIA, SECRETARIA)[m
[32m+[m			[32m.antMatchers("/medicos/dados", "/medicos/salvar", "/medicos/editar").hasAnyAuthority(SECRETARIA, ADMIN)[m
[32m+[m			[32m.antMatchers("/ano/inserir").hasAuthority(ADMIN)[m
[32m+[m			[32m.antMatchers("/emolumentos/**").hasAuthority(ADMIN)[m
[32m+[m			[32m.antMatchers("/grau/**").hasAuthority(ADMIN)[m
[32m+[m			[32m.antMatchers("/canditatos/**").hasAuthority(ADMIN)[m
[32m+[m			[32m.antMatchers("/arquitetos/**").hasAuthority(ADMIN)[m
[32m+[m			[32m.antMatchers("/diagnosticos/**").hasAuthority(SECRETARIA)[m
[32m+[m[41m			[m
[32m+[m			[32m// acessos privados pacientes[m
[32m+[m			[32m.antMatchers("/pacientes/**").hasAuthority(SECRETARIA)[m[41m			[m
[32m+[m[41m			[m
[32m+[m			[32m// acessos privados especialidades[m
[32m+[m			[32m.antMatchers("/especialidades/datatables/server/medico/*").hasAnyAuthority(SECRETARIA, ADMIN)[m
[32m+[m			[32m.antMatchers("/especialidades/titulo").hasAnyAuthority(SECRETARIA, ADMIN, SECRETARIA)[m
[32m+[m			[32m.antMatchers("/especialidades/**").hasAuthority(ADMIN)[m
[32m+[m[41m			[m
[32m+[m			[32m.anyRequest().authenticated()[m
[32m+[m			[32m.and()[m
[32m+[m				[32m.formLogin()[m
[32m+[m				[32m.loginPage("/login")[m
[32m+[m				[32m.defaultSuccessUrl("/", true)[m
[32m+[m				[32m.failureUrl("/login-error")[m
[32m+[m				[32m.permitAll()[m
[32m+[m			[32m.and()[m
[32m+[m				[32m.logout()[m
[32m+[m				[32m.logoutSuccessUrl("/")[m
[32m+[m			[32m.and()[m
[32m+[m				[32m.exceptionHandling()[m
[32m+[m				[32m.accessDeniedPage("/acesso-negado")[m
[32m+[m			[32m.and()[m
[32m+[m			[32m.sessionManagement().maximumSessions(1);[m
[32m+[m	[32m}[m
[32m+[m[41m	[m
[32m+[m	[32m@Override[m
[32m+[m	[32mprotected void configure(AuthenticationManagerBuilder auth) throws Exception {[m
[32m+[m		[32mauth.userDetailsService(service).passwordEncoder(new BCryptPasswordEncoder());[m
[32m+[m	[32m}[m
[32m+[m
[32m+[m[32m}[m
