[32m+[m[32m@RequestMapping("guiaPagamentos")[m
[32m+[m[32mpublic class GuiaPagamentoHistoricoController {[m
[32m+[m[41m	[m
[32m+[m	[32m@Autowired[m
[32m+[m	[32mprivate UsuarioRepository usuarioRepository;[m
[32m+[m	[32m@Autowired[m
[32m+[m	[32mprivate EmolumentosRepository emolumentoRepository;[m
[32m+[m	[32m@Autowired[m[41m [m
[32m+[m	[32mprivate AlunoRepository alunoRepository;[m
[32m+[m	[32m@Autowired[m
[32m+[m	[32mprivate AnoRepository anoRepository;[m
[32m+[m	[32m@Autowired[m
[32m+[m	[32mprivate GuiaPagamentoHisotricoRepository repository;[m
[32m+[m	[32m@GetMapping("/abrir")[m
[32m+[m	[32mpublic String abrir(RequisicaoNovaGuiaPagamentoHistorico requisicaoNovaGuiaPagamentoHistorico) {[m
[32m+[m		[32mreturn "guiaPagamento/inserir";[m
[32m+[m	[32m}[m
[32m+[m[41m	[m
[32m+[m	[32m@PostMapping("/novo")[m
[32m+[m	[32mpublic String novo(@Valid RequisicaoNovaGuiaPagamentoHistorico requisicao, RedirectAttributes attr) {[m
[32m+[m		[32mString email =SecurityContextHolder.getContext().getAuthentication().getName();[m
[32m+[m		[32mUsuario usuario = usuarioRepository.findByEmail(email);[m
[32m+[m		[32mGuiaPagamentoHistorico guiaPagamentoHistorico = requisicao.toGuiaPagamentoHistorico();[m
[32m+[m		[32mguiaPagamentoHistorico.setUsuario(usuario);[m
[32m+[m		[32mrepository.save(guiaPagamentoHistorico);[m
[32m+[m		[32mattr.addAttribute("sucesso", "dados gravados com sucesso");[m
[32m+[m		[32mreturn "redirect:/guiaPagamento/abrir";[m
[32m+[m	[32m}[m
