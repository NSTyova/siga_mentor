[32m+[m[32mpackage ao.co.temmais.arquitetura.repository;[m
[32m+[m
[32m+[m[32mimport java.util.Optional;[m
[32m+[m
[32m+[m[32mimport org.springframework.data.domain.Page;[m
[32m+[m[32mimport org.springframework.data.domain.Pageable;[m
[32m+[m[32mimport org.springframework.data.jpa.repository.JpaRepository;[m
[32m+[m[32mimport org.springframework.data.jpa.repository.Query;[m
[32m+[m[32mimport org.springframework.data.repository.query.Param;[m
[32m+[m[32mimport org.springframework.stereotype.Repository;[m
[32m+[m
[32m+[m[32mimport ao.co.temmais.arquitetura.modelo.Usuario;[m
[32m+[m
[32m+[m[32m@Repository[m
[32m+[m[32mpublic interface UsuarioRepository extends JpaRepository<Usuario, Long> {[m
[32m+[m
[32m+[m[41m	[m
[32m+[m	[32m@Query("select u from Usuario u where u.email like :email")[m
[32m+[m	[32mUsuario findByEmail(@Param("email") String email);[m
[32m+[m[41m	[m
[32m+[m[41m	[m
[32m+[m	[32m@Query("select u from Usuario u where u.email like :email AND u.ativo = true")[m
[32m+[m	[32mOptional<Usuario> findByEmailAndAtivo(String email);[m
[32m+[m[41m	[m
[32m+[m[41m	[m
[32m+[m	[32m@Query("select distinct u from Usuario u "[m
[32m+[m			[32m+ "join u.perfis p "[m
[32m+[m			[32m+ "where u.email like :search% OR p.desc like :search%")[m[41m [m
[32m+[m	[32mPage<Usuario> findByEmailOrPerfil(String search, Pageable pageable);[m
[32m+[m
[32m+[m	[32m@Query("select u from Usuario u "[m
[32m+[m			[32m+ "join u.perfis p "[m
[32m+[m			[32m+ "where u.id = :usuarioId AND p.id IN :perfisId")[m[41m [m
[32m+[m	[32mOptional<Usuario> findByIdAndPerfis(Long usuarioId, Long[] perfisId);[m
[32m+[m[32m}[m
