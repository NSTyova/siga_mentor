[32m+[m[32mpackage ao.co.temmais.arquitetura.repository.service;[m
[32m+[m
[32m+[m[32mimport java.util.List;[m
[32m+[m[32mimport java.util.Optional;[m
[32m+[m
[32m+[m[32mimport org.springframework.beans.factory.annotation.Autowired;[m
[32m+[m[32mimport org.springframework.security.core.authority.AuthorityUtils;[m
[32m+[m[32mimport org.springframework.security.core.userdetails.User;[m
[32m+[m[32mimport org.springframework.security.core.userdetails.UserDetails;[m
[32m+[m[32mimport org.springframework.security.core.userdetails.UserDetailsService;[m
[32m+[m[32mimport org.springframework.security.core.userdetails.UsernameNotFoundException;[m
[32m+[m[32mimport org.springframework.stereotype.Service;[m
[32m+[m[32mimport org.springframework.transaction.annotation.Transactional;[m
[32m+[m
[32m+[m[32mimport ao.co.temmais.arquitetura.modelo.Perfil;[m
[32m+[m[32mimport ao.co.temmais.arquitetura.modelo.Usuario;[m
[32m+[m[32mimport ao.co.temmais.arquitetura.repository.UsuarioRepository;[m
[32m+[m
[32m+[m
[32m+[m
[32m+[m[32m@Service[m
[32m+[m[32mpublic class UsuarioService implements UserDetailsService {[m
[32m+[m
[32m+[m	[32m@Autowired[m
[32m+[m	[32mprivate UsuarioRepository repository;[m
[32m+[m[41m	[m
[32m+[m	[32m@Transactional(readOnly = true)[m
[32m+[m	[32mpublic Usuario buscarPorEmail(String email) {[m
[32m+[m[41m		[m
[32m+[m		[32mreturn repository.findByEmail(email);[m
[32m+[m	[32m}[m
[32m+[m[41m	[m
[32m+[m[41m	[m
[32m+[m	[32m@Transactional(readOnly = true)[m
[32m+[m	[32mpublic Optional<Usuario> buscarPorEmailEAtivo(String email){[m
[32m+[m		[32mreturn repository.findByEmailAndAtivo(email);[m
[32m+[m	[32m}[m
[32m+[m[41m	[m
[32m+[m	[32mprivate String[] getAtuthorities(List<Perfil> perfis) {[m
[32m+[m		[32mString[] authorities = new String[perfis.size()];[m
[32m+[m		[32mfor (int i = 0; i < perfis.size(); i++) {[m
[32m+[m			[32mauthorities[i] = perfis.get(i).getDesc();[m
[32m+[m		[32m}[m
[32m+[m		[32mreturn authorities;[m
[32m+[m	[32m}[m
[32m+[m[41m	[m
[32m+[m	[32m// metodo usado para fazer a consulta pelo usuario e colocar eme na sesao[m
[32m+[m		[32m@Override @Transactional(readOnly = true)[m
[32m+[m		[32mpublic UserDetails loadUserByUsername(String username) throws UsernameNotFoundException {[m
[32m+[m			[32m// esta aqui so buscar por email[m
[32m+[m			[32m//Usuario usuario = buscarPorEmail(username);[m
[32m+[m			[32m// buscar por email atvio[m
[32m+[m			[32mUsuario usuario = buscarPorEmailEAtivo(username)[m
[32m+[m					[32m.orElseThrow(() -> new UsernameNotFoundException("Usuario " + username + "nao encontrado"));[m
[32m+[m			[32mreturn new User([m
[32m+[m				[32musuario.getEmail(),[m
[32m+[m				[32musuario.getSenha(),[m
[32m+[m				[32mAuthorityUtils.createAuthorityList(getAtuthorities(usuario.getPerfis()))[m
[32m+[m			[32m);[m
[32m+[m		[32m}[m
[32m+[m[41m		[m
[32m+[m[41m		[m
[32m+[m
[32m+[m[32m}[m
