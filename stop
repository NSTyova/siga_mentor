[32m+[m[32m/**[m
[32m+[m[32m * busca as especialidades com auto-complete[m
[32m+[m[32m */[m
[32m+[m[32m$("#especialidade").autocomplete({[m
[32m+[m[32m    source: function (request, response) {[m
[32m+[m[32m        $.ajax({[m
[32m+[m[32m            method: "GET",[m
[32m+[m[32m            url: "/especialidades/titulo",[m
[32m+[m[32m            data: {[m
[32m+[m[41m            [m	[32mtermo: request.term[m
[32m+[m			[32m},[m
[32m+[m[32m            success: function (data) {[m
[32m+[m[41m            [m	[32mresponse(data);[m
[32m+[m[32m            }[m
[32m+[m[32m        });[m
[32m+[m[32m    }[m
[32m+[m[32m});[m
[32m+[m
[32m+[m[32m/**[m
[32m+[m[32m * após a especialidade ser selecionado busca[m[41m [m
[32m+[m[32m * os médicos referentes e os adiciona na página com[m
[32m+[m[32m * radio[m
[32m+[m[32m */[m
[32m+[m[32m$('#especialidade').on('blur', function() {[m
[32m+[m[32m    $('div').remove(".custom-radio");[m
[32m+[m	[32mvar titulo = $(this).val();[m
[32m+[m	[32mif ( titulo != '' ) {[m[41m			[m
[32m+[m		[32m$.get( "/medicos/especialidade/titulo/" + titulo , function( result ) {[m
[32m+[m[41m				[m
[32m+[m			[32mvar ultimo = result.length - 1;[m[41m [m
[32m+[m[41m			[m
[32m+[m			[32m$.each(result, function (k, v) {[m
[32m+[m[41m				[m
[32m+[m				[32mif ( k == ultimo ) {[m
[32m+[m	[41m    [m			[32m$("#medicos").append([m[41m [m
[32m+[m	[41m    [m				[32m '<div class="custom-control custom-radio">'[m[41m	[m
[32m+[m	[41m    [m				[32m+  '<input class="custom-control-input" type="radio" id="customRadio'+ k +'" name="medico.id" value="'+ v.id +'" required>'[m
[32m+[m						[32m+  '<label class="custom-control-label" for="customRadio'+ k +'">'+ v.nome +'</label>'[m
[32m+[m						[32m+  '<div class="invalid-feedback">Médico é obrigatório</div>'[m
[32m+[m						[32m+'</div>'[m
[32m+[m	[41m    [m			[32m);[m
[32m+[m				[32m} else {[m
[32m+[m	[41m    [m			[32m$("#medicos").append([m[41m [m
[32m+[m	[41m    [m				[32m '<div class="custom-control custom-radio">'[m[41m	[m
[32m+[m	[41m    [m				[32m+  '<input class="custom-control-input" type="radio" id="customRadio'+ k +'" name="medico.id" value="'+ v.id +'" required>'[m
[32m+[m						[32m+  '<label class="custom-control-label" for="customRadio'+ k +'">'+ v.nome +'</label>'[m
[32m+[m						[32m+'</div>'[m
[32m+[m	[41m        [m		[32m);[m[41m	            				[m
[32m+[m				[32m}[m
[32m+[m		[32m    });[m
[32m+[m		[32m});[m
[32m+[m	[32m}[m
[32m+[m[32m});[m[41m	[m
[32m+[m
[32m+[m[32m/**[m[41m [m
[32m+[m[32m * busca os horários livres para consulta conforme a data e o médico.[m
[32m+[m[32m * os horários são adicionados a página como um select:option.[m[41m	[m
[32m+[m[32m*/[m
[32m+[m[32m$('#data').on('blur', function () {[m
[32m+[m	[32m$("#horarios").empty();[m
[32m+[m[32m    var data = $(this).val();[m
[32m+[m[32m    var medico = $('input[name="medico.id"]:checked').val();[m
[32m+[m[32m    if (Date.parse(data)) {[m
[32m+[m[32m        $.get('/agendamentos/horario/medico/'+ medico + '/data/' + data , function( result ) {[m
[32m+[m[32m            $.each(result, function (k, v) {[m
[32m+[m[32m                $("#horarios").append([m[41m [m
[32m+[m[32m                    '<option class="op" value="'+ v.id +'">'+ v.horaMinuto + '</option>'[m
[32m+[m[32m                );[m[41m	            			[m
[32m+[m[32m            });[m
[32m+[m[32m        });[m
[32m+[m[32m    }[m
[32m+[m[32m});[m
[32m+[m
[32m+[m[32m/**[m
[32m+[m[32m * Datatable histórico de consultas[m
[32m+[m[32m*/[m
[32m+[m[32m$(document).ready(function() {[m
[32m+[m[32m    moment.locale('pt-BR');[m
[32m+[m[32m    var table = $('#table-paciente-historico').DataTable({[m
[32m+[m[32m        searching : false,[m
[32m+[m[32m        lengthMenu : [ 5, 10 ],[m
[32m+[m[32m        processing : true,[m
[32m+[m[32m        serverSide : true,[m
[32m+[m[32m        responsive : true,[m
[32m+[m[32m        order: [2, 'desc'],[m
[32m+[m[32m        ajax : {[m
[32m+[m[32m            url : '/agendamentos/datatables/server/historico',[m
[32m+[m[32m            data : 'data'[m
[32m+[m[32m        },[m
[32m+[m[32m        columns : [[m
[32m+[m[32m            {data : 'id'},[m
[32m+[m[32m            {data : 'paciente.nome'},[m
[32m+[m[32m            {data: 'dataConsulta', render:[m
[32m+[m[32m                function( dataConsulta ) {[m
[32m+[m[32m                    return moment(dataConsulta).format('LLL');[m
[32m+[m[32m                }[m
[32m+[m[32m            },[m
[32m+[m[32m            {data : 'medico.nome'},[m
[32m+[m[32m            {data : 'especialidade.titulo'},[m
[32m+[m[32m            {orderable : false,	data : 'id', "render" : function(id) {[m
[32m+[m[32m                    return '<a class="btn btn-success btn-sm btn-block" href="/agendamentos/editar/consulta/'[m
[32m+[m[32m                            + id + '" role="button"><i class="fas fa-edit"></i></a>';[m
[32m+[m[32m                }[m
[32m+[m[32m            },[m
[32m+[m[32m            {orderable : false,	data : 'id', "render" : function(id) {[m
[32m+[m[32m                    return '<a class="btn btn-danger btn-sm btn-block" href="/agendamentos/excluir/consulta/'[m
[32m+[m[32m                    + id +'" role="button" data-toggle="modal" data-target="#confirm-modal"><i class="fas fa-times-circle"></i></a>';[m
[32m+[m[32m                }[m
[32m+[m[32m            }[m
[32m+[m[32m        ][m
[32m+[m[32m    });[m
[32m+[m[32m});[m
