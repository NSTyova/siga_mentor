[32m+[m	[32mmoment.locale('pt-BR');[m
[32m+[m	[32mvar table = $('#table-usuarios').DataTable({[m
[32m+[m		[32msearching : true,[m
[32m+[m		[32mlengthMenu : [ 5, 10 ],[m
[32m+[m		[32mprocessing : true,[m
[32m+[m		[32mserverSide : true,[m
[32m+[m		[32mresponsive : true,[m
[32m+[m		[32majax : {[m
[32m+[m			[32murl : '/u/datatables/server/usuarios',[m
[32m+[m			[32mdata : 'data'[m
[32m+[m		[32m},[m
[32m+[m		[32mcolumns : [[m
[32m+[m				[32m{data : 'id'},[m
[32m+[m				[32m{data : 'email'},[m
[32m+[m				[32m{	data : 'ativo',[m[41m [m
[32m+[m					[32mrender : function(ativo) {[m
[32m+[m						[32mreturn ativo == true ? 'Sim' : 'Não';[m
[32m+[m					[32m}[m
[32m+[m				[32m},[m
[32m+[m				[32m{	data : 'perfis',[m[41m									 [m
[32m+[m					[32mrender : function(perfis) {[m
[32m+[m						[32mvar aux = new Array();[m
[32m+[m						[32m$.each(perfis, function( index, value ) {[m
[32m+[m							[32m  aux.push(value.desc);[m
[32m+[m						[32m});[m
[32m+[m						[32mreturn aux;[m
[32m+[m					[32m},[m
[32m+[m					[32morderable : false,[m
[32m+[m				[32m},[m
[32m+[m				[32m{	data : 'id',[m[41m	[m
[32m+[m					[32mrender : function(id) {[m
[32m+[m						[32mreturn ''.concat('<a class="btn btn-success btn-sm btn-block"', ' ')[m
[32m+[m								[32m .concat('href="').concat('/u/editar/credenciais/usuario/').concat(id, '"', ' ')[m[41m [m
[32m+[m								[32m .concat('role="button" title="Editar" data-toggle="tooltip" data-placement="right">', ' ')[m
[32m+[m								[32m .concat('<i class="fas fa-edit"></i></a>');[m
[32m+[m					[32m},[m
[32m+[m					[32morderable : false[m
[32m+[m				[32m},[m
[32m+[m				[32m{	data : 'id',[m[41m	[m
[32m+[m					[32mrender : function(id) {[m
[32m+[m						[32mreturn ''.concat('<a class="btn btn-info btn-sm btn-block"', ' ')[m[41m [m
[32m+[m								[32m .concat('id="dp_').concat(id).concat('"', ' ')[m[41m [m
[32m+[m								[32m .concat('role="button" title="Editar" data-toggle="tooltip" data-placement="right">', ' ')[m
[32m+[m								[32m .concat('<i class="fas fa-edit"></i></a>');[m
[32m+[m					[32m},[m
[32m+[m					[32morderable : false[m
[32m+[m				[32m}[m
[32m+[m		[32m][m
[32m+[m	[32m});[m
[32m+[m[41m	[m
[32m+[m[32m    $('#table-usuarios tbody').on('click', '[id*="dp_"]', function () {[m
[32m+[m[41m    [m	[32mvar data = table.row($(this).parents('tr')).data();[m
[32m+[m[41m    [m	[32mvar aux = new Array();[m
[32m+[m		[32m$.each(data.perfis, function( index, value ) {[m
[32m+[m			[32m  aux.push(value.id);[m
[32m+[m		[32m});[m
[32m+[m		[32mdocument.location.href = '/u/editar/dados/usuario/' + data.id + '/perfis/' + aux;[m
[32m+[m[32m    } );[m[41m	[m
[32m+[m[41m	[m
[32m+[m[32m});[m[41m	[m
