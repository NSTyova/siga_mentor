[32m+[m[32mpackage ao.co.temmais.arquitetura.datatables;[m
[32m+[m
[32m+[m[32mimport java.util.LinkedHashMap;[m
[32m+[m[32mimport java.util.Map;[m
[32m+[m
[32m+[m[32mimport javax.servlet.http.HttpServletRequest;[m
[32m+[m
[32m+[m[32mimport org.springframework.data.domain.Page;[m
[32m+[m[32mimport org.springframework.data.domain.PageRequest;[m
[32m+[m[32mimport org.springframework.data.domain.Pageable;[m
[32m+[m[32mimport org.springframework.data.domain.Sort;[m
[32m+[m[32mimport org.springframework.stereotype.Component;[m
[32m+[m
[32m+[m[32m@Component[m
[32m+[m[32mpublic class Datatables {[m
[32m+[m
[32m+[m	[32mprivate HttpServletRequest request;[m
[32m+[m	[32mprivate String[] colunas;[m[41m	[m
[32m+[m
[32m+[m	[32mpublic Datatables() {[m
[32m+[m		[32msuper();[m
[32m+[m	[32m}[m
[32m+[m[41m	[m
[32m+[m	[32mpublic Map<String, Object> getResponse(Page<?> page) {[m[41m		[m
[32m+[m		[32mMap<String, Object> json = new LinkedHashMap<>();[m
[32m+[m		[32mjson.put("draw", draw());[m
[32m+[m		[32mjson.put("recordsTotal", page.getTotalElements());[m
[32m+[m		[32mjson.put("recordsFiltered", page.getTotalElements());[m
[32m+[m		[32mjson.put("data", page.getContent());[m
[32m+[m		[32mreturn json;[m
[32m+[m	[32m}[m[41m	[m
[32m+[m
[32m+[m	[32mpublic HttpServletRequest getRequest() {[m
[32m+[m		[32mreturn request;[m
[32m+[m	[32m}[m
[32m+[m
[32m+[m	[32mpublic void setRequest(HttpServletRequest request) {[m
[32m+[m		[32mthis.request = request;[m
[32m+[m	[32m}[m
[32m+[m
[32m+[m	[32mpublic String[] getColunas() {[m
[32m+[m		[32mreturn colunas;[m
[32m+[m	[32m}[m
[32m+[m
[32m+[m	[32mpublic void setColunas(String[] colunas) {[m
[32m+[m		[32mthis.colunas = colunas;[m
[32m+[m	[32m}[m
[32m+[m
[32m+[m	[32mprivate int draw() {[m
[32m+[m		[32mreturn Integer.parseInt(this.request.getParameter("draw"));[m
[32m+[m	[32m}[m
[32m+[m[41m	[m
[32m+[m	[32mprivate int start() {[m
[32m+[m		[32mreturn Integer.parseInt(this.request.getParameter("start"));[m
[32m+[m	[32m}[m
[32m+[m[41m	[m
[32m+[m	[32mpublic int getLength() {[m
[32m+[m		[32mreturn Integer.parseInt(this.request.getParameter("length"));[m
[32m+[m	[32m}[m
[32m+[m
[32m+[m	[32mpublic int getCurrentPage() {[m
[32m+[m		[32mreturn start() / getLength();[m
[32m+[m	[32m}[m
[32m+[m
[32m+[m	[32mpublic String getColumnName() {[m
[32m+[m		[32mint iCol = Integer.parseInt(this.request.getParameter("order[0][column]"));[m
[32m+[m		[32mreturn this.colunas[iCol];[m
[32m+[m	[32m}[m[41m	[m
[32m+[m
[32m+[m	[32mpublic Sort.Direction getDirection() {[m
[32m+[m		[32mString order = this.request.getParameter("order[0][dir]");[m
[32m+[m		[32mSort.Direction sort = Sort.Direction.ASC;[m
[32m+[m		[32mif (order.equalsIgnoreCase("desc")) {[m
[32m+[m			[32msort = Sort.Direction.DESC;[m
[32m+[m		[32m}[m
[32m+[m		[32mreturn sort;[m
[32m+[m	[32m}[m
[32m+[m
[32m+[m	[32mpublic String getSearch() {[m[41m		[m
[32m+[m		[32mreturn this.request.getParameter("search[value]");[m
[32m+[m	[32m}[m
[32m+[m[41m	[m
[32m+[m	[32mpublic Pageable getPageable() {[m
[32m+[m		[32mreturn PageRequest.of(getCurrentPage(), getLength(), getDirection(), getColumnName());[m
[32m+[m	[32m}[m[41m [m
[32m+[m[32m}[m
