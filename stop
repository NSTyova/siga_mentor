[32m+[m		[32mrepository.save(aluno);[m
[32m+[m		[32mattr.addFlashAttribute("sucesso", "Operação realizada com sucesso!");[m
[32m+[m		[32mreturn "redirect:/alunos/abrir";[m
[32m+[m	[32m}[m
